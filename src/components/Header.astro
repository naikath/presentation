---
import { Image } from 'astro:assets';
---

<header>
	<Image src='/foxPlush.png' alt='Profile Picture' width='200' height='200' />
	<h1>Title</h1>
</header>

<style>
	header {
		display: flex;
		justify-content: space-evenly;
		align-items: center;
		text-align: center;
	}
	header h1 {
		display: flex;
		flex-wrap: wrap;
	}

	img {
		filter: drop-shadow(1px 1px 5px white);
		aspect-ratio: 1;
		height: 200px;
		object-fit: contain;
	}
</style>
